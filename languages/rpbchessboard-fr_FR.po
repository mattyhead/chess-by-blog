#: templates/adminpage/optionsgeneral.php:267
msgid "Position of the navigation board"
msgstr "Position de l'échiquier de navigation"

#: templates/adminpage/optionsgeneral.php:277
#: templates/adminpage/optionsgeneral.php:278
msgid "No navigation board"
msgstr "Pas d'échiquier de navigation"

#: templates/adminpage/optionsgeneral.php:289
#: templates/adminpage/optionsgeneral.php:290
